{
  "name": "spantree-java7",
  "version": "0.2.6",
  "author": "spantree",
  "summary": "Oracle Java8 Puppet Module",
  "license": "Apache 2.0",
  "source": "https://github.com/Spantree/puppet-java8.git",
  "project_page": "https://github.com/spantree/puppet-java8",
  "issues_url": "https://github.com/spantree/puppet-java8/issues",
  "description": "This module manages Oracle Java8. Tested on Ubuntu",
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04"
      ]
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/apt",
      "version_range": ">= 1.0.1"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_range": ">= 4.0.0 <5.0.0"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=3.2.0 <4.0.0"
    }
  ]
}
